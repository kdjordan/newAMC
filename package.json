    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "autoprefixer": "^9.6.1",
    "validator": "^11.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "flatpickr": "^4.6.2"
  }
}
