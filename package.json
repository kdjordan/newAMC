    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.0.0",
    "dompurify": "^1.0.11",
    "sanitize-html": "^2.3.2",
    "dotenv": "^8.1.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "md5": "^2.2.1",
    "mongodb": "^3.3.0",
    "nodemon": "^1.19.1",
    "concat": "^1.0.3",
